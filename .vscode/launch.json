{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug with XVFB",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/xvfb-run",
      "args": [
        "${workspaceFolder}/build/bin/AudioProcessingProject"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build/bin",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
