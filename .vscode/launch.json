{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug AudioProcessingProject",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/bin/AudioProcessingProject", // Caminho para o binário
        "args": [], // Argumentos para o programa, se necessário
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/build/bin", // Diretório de trabalho (onde está o binário)
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb", // Certifique-se de ter o gdb instalado
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  